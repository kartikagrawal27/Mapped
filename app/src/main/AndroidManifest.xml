<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mapped">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mapped_launcher3"
        android:label="Mapped"
        android:supportsRtl="true"
        android:theme="@style/FullscreenTheme">
        <activity android:name=".CheckingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".CalendarActivity"
            android:label="@string/title_activity_calendar"
            android:theme="@style/AppTheme">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity
            android:name=".EventDetailsActivity"
            android:label="@string/title_activity_event_details"
            android:parentActivityName=".CalendarActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mapped.CalendarActivity" />
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Login Activity"
            android:theme="@style/AppTheme" />
        <activity android:name=".ViewPagerActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/title_activity_checking"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>